{
  "name": "interbank-frontend-frame",
  "version": "0.0.1",
  "author": "luventa <luventa@outlook.com>",
  "description": "HSS Client frame based on Electron and VueJs",
  "license": "MIT",
  "main": "./dist/main.js",
  "scripts": {
    "start": "npm run help",
    "analyze": "cross-env NODE_ENV=production TARGET=web MODE=analyze node scripts/index.js",
    "clean": "rimraf dist build",
    "clean:dll": "rimraf src/static/dll",
    "dev": "cross-env NODE_ENV=development TARGET=web MODE=dev node scripts/index.js",
    "dev:client": "cross-env NODE_ENV=development TARGET=electron MODE=dev node scripts/index.js",
    "dll": "npm run clean:dll && webpack --config ./scripts/webpack.dll.conf.js",
    "build": "cross-env NODE_ENV=testing TARGET=web MODE=build node scripts/index.js",
    "build:client": "cross-env NODE_ENV=testing TARGET=electron MODE=build node scripts/index.js",
    "dist": "cross-env NODE_ENV=production TARGET=web MODE=build node scripts/index.js",
    "dist:client": "cross-env NODE_ENV=production TARGET=electron MODE=build node scripts/index.js",
    "client:unpack": "electron-builder --dir",
    "client:win32": "electron-builder --win --ia32",
    "client:win64": "electron-builder --win --x64",
    "e2e": "npm run pack && mocha test/e2e",
    "help": "node ./scripts/help.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "test": "npm run unit && npm run e2e",
    "package": "node scripts/mkpkg.js",
    "postinstall": "npm run dll"
  },
  "build": {
    "productName": "interbank-frontend-frame",
    "appId": "oneconnect.interbank.client",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "icon": "src/electron/logo_256.png",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "hover.css": "^2.3.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "archiver": "^3.0.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "compression-webpack-plugin": "^2.0.0",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.3",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "es6-promise": "^4.2.5",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "json-loader": "^0.5.7",
    "koa": "^2.6.2",
    "koa-convert": "^1.2.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa2-connect": "^1.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "opn": "^5.4.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.5"
  }
}
