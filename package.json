{
  "name": "vue-frontend-boilerpalte",
  "version": "1.0.2",
  "author": "luventa <luventa@outlook.com>",
  "description": "Frontend boilerpalte for web and electron uses Webpack4 & Vue.js",
  "license": "MIT",
  "main": "./dist/main.js",
  "scripts": {
    "asar": "node scripts/mkasar.js",
    "start": "npm run help",
    "analyze": "cross-env NODE_ENV=production TARGET=web MODE=analyze node scripts/index.js",
    "clean": "rimraf dist build",
    "clean:dll": "rimraf src/static/dll",
    "dev": "cross-env NODE_ENV=development TARGET=web MODE=dev node scripts/index.js",
    "dev:client": "cross-env NODE_ENV=development TARGET=electron MODE=dev node scripts/index.js",
    "dll": "npm run clean:dll && webpack --config ./scripts/webpack.dll.conf.js",
    "build": "cross-env NODE_ENV=testing TARGET=web MODE=build node scripts/index.js",
    "build:client": "cross-env NODE_ENV=testing TARGET=electron MODE=build node scripts/index.js",
    "dist": "cross-env NODE_ENV=production TARGET=web MODE=build node scripts/index.js",
    "dist:client": "cross-env NODE_ENV=production TARGET=electron MODE=build node scripts/index.js",
    "client:unpack": "electron-builder -c ./scripts/builder.conf.js --dir",
    "client:test": "electron-builder -c ./scripts/builder.conf.js testing",
    "client:publish": "electron-builder -c ./scripts/builder.conf.js production",
    "e2e": "npm run pack && mocha test/e2e",
    "help": "node ./scripts/help.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "lint:style": "stylelint src/client/**/*.{vue,css,sass,scss} --cache --fix",
    "test": "npm run unit && npm run e2e",
    "package": "node scripts/mkpkg.js",
    "postinstall": "npm run dll"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "hover.css": "^2.3.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "archiver": "^3.0.0",
    "asar": "^2.0.3",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "compression-webpack-plugin": "^2.0.0",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^3.6.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^7.2.4",
    "electron-builder": "^21.0.15",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-store": "^5.0.0",
    "electron-suites": "^1.0.4",
    "electron-updater": "^4.0.6",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hash.js": "^1.1.7",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.1",
    "json-loader": "^0.5.7",
    "koa": "^2.6.2",
    "koa-convert": "^1.2.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "koa2-connect": "^1.0.2",
    "log4js": "^6.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "opn": "^5.4.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.5"
  }
}
